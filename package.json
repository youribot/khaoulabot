{
  "name": "khaoulabot.app",
  "version": "0.0.1",
  "description": "khaoulabot.app, with React and Redux",
  "main": "src/app.js",
  "author": "Nikhil M <niksmac@lightrains.com>",
  "license": "MIT",
  "jest": {
    "testRegex": ".*\\.test.js",
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "electron .",
    "buildassets": "gulp",
    "test": "jest",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "dependencies": {
    "babel-jest": "^18.0.0",
    "config": "^1.24.0",
    "eslint-config-defaults": "^9.0.0",
    "hapi": "^16.1.0",
    "hapi-routes": "^2.2.3",
    "immutable": "^3.8.1",
    "inert": "^4.1.0",
    "ipfs-api": "^12.1.4",
    "lodash": "^4.17.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "semantic-ui": "^2.2.7"
  },
  "private": true,
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.5.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.5.0",
    "electron-prebuilt-compile": "^1.1.3",
    "eslint": "^3.13.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.0.0",
    "gulp-electron": "^0.1.3",
    "jest": "^18.1.0",
    "redux-devtools-extension": "^1.0.0"
  }
}
